
	<html>
	<head>
	<style>
		tbody{
			margin: 0in 7in 7in 4in;
			width: 11in;
			height: 11in;
		}

		*{
			font-family:calibri;
			font-size:14px;
		}

		th {
			background-color:blue;
			color:white;
			font-weight:bold;
		}

		td {
			vertical-align:top;
		}

		.store-info div {
			font-size:1.2em;
		}

		.store-info div.company-name {
			font-size:1.5em;
			font-weight: bold;
		}

		table.order-info td {

			padding: 10px 4px 2px 4px;
		}

		table.order-info tr td.label {
			font-weight:bold;
			text-align:right;
			border-right:solid 1px #c0c0c0;

		}

		table.order-info tr td.label.first {

		}

		table.order-info tr td.label.last {

		}

		table.line-items {
			margin-top:0.1in;
			padding: 0.1in 0in 0.1in 0in;
		}

		table.line-items th {
			padding:2px;
		}

		table.footer
		{
			border-top:solid 1px #707070;
		}

		table.footer td.label
		{
			font-weight:bold;
			text-align:right;
		}

		td.notes
		{
			padding: 0.1in;
			font-style: italic;
		}

		.barcode
		{
			font-family:"Free 3 of 9 Extended";
			font-size:48pt;
		}


	</style>
	</head>

	<body>

	<div style="height:10in; width:10in; ">

	<!-- Order Header - THIS SECTION CAN BE MODIFIED AS NEEDED -->

	<table cellspacing=0 cellpadding="7" border=0 style="width:10in" >


	<thead>
		<tr>
			<th colspan="3">
				Employee Payslip
			</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td colspan="2" style="width:4.5in" class="store-info">
				<div class="company-name">



				</div>
				<div>MDNS DB<br/>Accra Ghana</div>
			</td>
			<td style="width:3.5in;" align="right" valign="top">

			</td>
		</tr>
		<tr>
			<td style="height:0.15in"></td>
		</tr>
		<tr>
			<td align="right" style="width:1in">
			<div>
				<b>Name:</b>
		    </div>
			<div>
				<b>Tel:</b>
		    </div>
			</td>

			<td style="width:3.5in; font-size:14px">
				<div>


						{{casual.name}}

				{{--  @  --}}

				</div>

				<div>

						{{casual.contact}}


				<br/>



				<br/>
				</div>


			</td>

			<td style="width:2.5in","float:right" float:right;>
				<table cellspacing="4" border="0" class="order-info">
				<tr>

					<td align="right" class="label first">Month </td>
					<td>{{rate.month}}</td>

				</tr>
				<tr>
					<td align="right" class="label">SSNIT Rate</td>
					<td>5 %</td>
				</tr>
				<tr>
					<td align="right" class="label">Tax rate</td>
					<td>5 %</td>
				</tr>
				<tr>
					<td align="right" class="label last">Date</td>
					<td>4/18/2012</td>
				</tr>
				</table>
			</td>
		</tr>
	</tbody>
	</table>



	<table cellspacing=0 cellpadding="2" border="0" style="width:100%" class="line-items">
	<thead>

	<!-- Order Items Header - THIS SECTION CAN BE MODIFIED AS NEEDED -->

		<tr>
			<th align="left" style="width:1.5in" class="sku">
				Tax deduction
			</th>
			<th align="left">
				SSNIT deduction
			</th>
			<th align="right" style="width:0.9in" class="price">
				Transport Allowance
			</th>
			<th align="center" style="width:0.9in">
				               Basic
			</th>
			<th align="right" style="width:0.9in" class="price">
				Overtime
			</th>
		</tr>

	<!-- END Order Items Header -->

	</thead>
	<tbody>

	<!-- Order Items - THIS SECTION CAN BE MODIFIED AS NEEDED -->
		<tr>
			<td class="sku">{{rate.GRA}}</td>
			<td>{{rate.SSNIT}}</td>
			<td align="right" class="price">{{rate.tans_allow}}</td>
			<td align="center">{{rate.basic}}</td>
			<td align="right" class="price">{{rate.overtime}}</td>
		</tr>
	<!-- END Order Items -->

	</tbody>
	</table>

	<!-- Order Footer - THIS SECTION CAN BE MODIFIED AS NEEDED -->

	<table cellspacing=0 cellpadding="2" border="0" style="width:100%" class="footer">
	<tbody>
		<tr>
			<td rowspan="3" class="notes" >
				Deductions
			</td>
			<td align="right" style="width:1in" class="label price">
				Basic:
			</td>
			<td style="width:0.75in" align="right" class="price">  {{rate.basic}}</td>

		</tr>
		<tr>
			<td align="right" class="label price">
				Gross:
			</td>
			<td style="width:1in" align="right" class="price">{{rate.gross}}</td>
		</tr>
		<tr>
			<td align="right" class="label price">
				Net payable:
			</td>
			<td style="width:0.75in" align="right" class="price">{{rate.net}}</td>
		</tr>
	</tbody>
	</table>
		<div class="card-footer">
						<button type="submit" class="btn btn-primary"   align="left">Print</button>
					</div>
	<!-- END Order Footer -->

	</div>

	</body>
	</html>






